4	4	Charmander	Fire	NaN	309	39	52	43	60	50	65	1	False
speed_mean = data["Speed"].mean()
speed_mean
68.2775
def set_speed(value):
    if value < speed_mean:
        return "Low speed"
    elif value > speed_mean:
        return "High speed"
    else:
        return "Same speed"
data["Speed_high_low"] = data["Speed"].apply(set_speed)
data

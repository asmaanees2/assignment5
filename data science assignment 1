HP_mean = data["HP"].mean()
HP_mean
69.25875
def set_hp(value):
    if value < HP_mean:
        return "Low"
    elif value > HP_mean:
        return "High"
    else:
        return "Same"
data["HP_high_low"] = data["HP"].apply(set_hp)
data
